<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>GetOpoint</servlet-name>
        <servlet-class>Tools.GetO</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetOpoint</servlet-name>
        <url-pattern>/GetOpoint</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetDpoint</servlet-name>
        <servlet-class>Tools.GetD</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetDpoint</servlet-name>
        <url-pattern>/GetDpoint</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>GetAllOJH</servlet-name>
        <servlet-class>Tools.GetAllOJH</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetAllOJH</servlet-name>
        <url-pattern>/GetAllOJH</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetAllDJH</servlet-name>
        <servlet-class>Tools.GetAllDJH</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetAllDJH</servlet-name>
        <url-pattern>/GetAllDJH</url-pattern>
    </servlet-mapping>

</web-app>